{
  "compilerOptions": {
    "target": "es2020",
    "lib": ["es2020"],
    "allowJs": false,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "outDir": "./dist"
  },
  "files": [
    "src/server.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
